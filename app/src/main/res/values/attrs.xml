<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MySeekBar">
        <attr name="sb_max" format="integer" />
        <attr name="sb_progress" format="integer" />
        <attr name="sb_per_progress" format="dimension" />
        <attr name="sb_progress_color" format="color|reference" />
        <attr name="sb_progress_background_color" format="color|reference" />
        <attr name="sb_mode" format="enum">
            <enum name="avg" value="1" />
            <enum name="tile" value="2" />
        </attr>
    </declare-styleable>


</resources>